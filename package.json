{
  "name": "sql-repl",
  "version": "0.1.0",
  "description": "Just a SQL REPL for web.",
  "scripts": {
    "install:client": "cd client && pnpm install",
    "install:server": "cd server && pnpm install",
    "postinstall": "run-p install:*",
    "dev:client": "npm run dev --prefix client",
    "dev:server": "npm run dev --prefix server",
    "start": "run-p dev:*",
    "build:client": "npm run build --prefix client",
    "build:server": "npm run build --prefix server",
    "build": "run-p build:*",
    "serve": "npm run serve --prefix server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xiaoluoboding/sql-repl.git"
  },
  "author": "xiaoluoboding<xiaolubooding@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xiaoluoboding/sql-repl/issues"
  },
  "homepage": "https://github.com/xiaoluoboding/sql-repl#readme",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
